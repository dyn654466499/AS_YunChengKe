<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <solid android:color="#ffffff" />

            <corners
                android:bottomLeftRadius="@dimen/title_corner"
                android:topLeftRadius="@dimen/title_corner" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item >
        <!--边框里面背景颜色 白色-->
        <shape>
            <solid android:color="@color/catering_title_color" />
            <corners
