<resources>

    <!-- AppAnimation动画 -->
    <style name="AppAnimation" />

    <!-- BaseDialog动画 -->
    <style name="AppAnimation.Dialog" parent="@android:style/Animation.Dialog" >

    </style>

    <!-- BaseActivity动画 -->
    <style name="AppAnimation.Activity" parent="@android:style/Animation.Activity" >
        <item name="android:activityOpenEnterAnimation">@anim/splash_open_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/splash_open_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/splash_close_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/splash_close_exit</item>

        <item name="android:taskOpenEnterAnimation">@anim/splash_open_enter</item>
        <item name="android:taskOpenExitAnimation">@anim/splash_open_exit</item>
        <item name="android:taskCloseEnterAnimation">@anim/splash_close_enter</item>
        <item name="android:taskCloseExitAnimation">@anim/splash_close_exit</item>
    </style>

    <!-- MainActivity动画 -->
    <style name="MainAnimation" parent="@android:style/Animation.Activity" >
        <item name="android:activityOpenEnterAnimation">@anim/main_open_enter</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@anim/splash_close_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/splash_close_exit</item>

        <item name="android:taskOpenEnterAnimation">@anim/main_open_enter</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
    </style>

    <!-- SplashActivity动画 -->
    <style name="AppAnimation.Splash" parent="AppAnimation.Activity" >

    </style>


    <!-- SegmentButton -->
    <style name="SegmentRadioButton">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@drawable/button_text_color</item>
        <item name="android:minHeight">33dp</item>
        <item name="android:minWidth">70dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>

    <style name="ActivityTransparent">
        <item name="android:windowBackground">@color/activitytransparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <!--机票-->
    <style name="chat_text_date_style">
        <item name="android:padding">2dp</item>
        <item name="android:textColor">#8f8f8f</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="chat_content_date_style">
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="chat_text_name_style">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="noAnimationTheme" parent="android:Theme">
        <item name="android:windowAnimationStyle">@style/noAnimation</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="noAnimation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>

    <style name="animation_dropdown" mce_bogus="1" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>

    <style name="MyDialogStyle2" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="form_text" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/black</item>
        <item name="android:minWidth">@dimen/text_min_width</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="form_text1" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/black</item>
        <item name="android:minWidth">@dimen/text_min_width</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <style name="form_edittext" parent="@android:style/Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="form_edittext1" parent="@android:style/Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="MySettingText">
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_weight">6</item>
    </style>

    <style name="radio_bottom">
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="AnimTop" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_top_in</item>
        <item name="android:windowExitAnimation">@anim/push_top_out</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <!-- 控件宽高的样式 -->
    <style name="all_fill_parent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="all_wrap_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="width_fill_parent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="height_fill_parent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="closedialog">
        <item name="android:windowContentOverlay">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@drawable/bg_popupwindow</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- 模糊 -->
    </style>


    <style name="PopupWindowAnimation">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowBackground">@drawable/bg_popupwindow</item>-->
    </style>

    <style name="pop">
        <item name="android:windowEnterAnimation">@anim/time_popup_enter</item>
        <item name="android:windowExitAnimation">@anim/time_popup_exit</item>
    </style>

</resources>
