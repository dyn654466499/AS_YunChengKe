<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">  
    <item android:id="@android:id/background">  
        <shape>  
            <corners android:radius="5dp" />  
            <gradient android:startColor="@color/white" android:centerColor="@color/white" android:endColor="@color/white" android:angle="270.0" android:centerY="2.0"  />  
        </shape>  
    </item>  
    <!-- #ff000000 -->
    <item android:id="@android:id/progress">  
        <clip>  
            <shape>  
                <corners android:radius="5dp" />  
                <gradient android:startColor="#ff33b5e5" android:centerColor="#ff33b5e5" android:endColor="#ff33b5e5" android:angle="270.0" android:centerY="2.0"  />  
            </shape>  
        </clip>  
    </item>  
</layer-list>  